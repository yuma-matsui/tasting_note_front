    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "web-vitals": "^2.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run fix:eslint",
    "*": "npm run fix:prettier"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
